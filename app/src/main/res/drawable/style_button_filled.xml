<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="100dp" />
            <solid android:color="@color/material_dynamic_secondary20" />
            <stroke android:width="1dp" android:color="@color/material_dynamic_secondary20" />
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="100dp" />
            <solid android:color="@color/material_dynamic_secondary60" />
            <stroke android:width="1dp" android:color="@color/material_dynamic_secondary60" />
        </shape>
    </item>
</selector>